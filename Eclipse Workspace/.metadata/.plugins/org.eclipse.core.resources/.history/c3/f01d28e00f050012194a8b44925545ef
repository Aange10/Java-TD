package javatd;

public class JavaTD {

	public static void main(String[] args) {
		
		for (int x = 0; x < 4; x++) {
			System.out.print ( (int) (100 + (100 * ((x + 1) * 0.1)) * (x + 1)) + ", ");
		}
		
		System.out.println("");
		
		for (int x = 0; x < 4; x++) {
			System.out.print( (int) ((700 * 0.5) * (x + 1)) + ", ");
		}
	}
}
